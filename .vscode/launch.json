	"version": "0.2.0",
	"configurations": [{
		"type": "node",
		"request": "launch",
		"name": "RUN Local API ",
		"sourceMaps": true,
		"program": "${workspaceFolder}/src/app.js",
		"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
		"cwd": "${workspaceFolder}",
		"port": 5858,
		"console": "integratedTerminal",
		"env": {
			"NODE_ENV": "local"
		},
		"runtimeArgs": ["--inspect=5858"],
		"timeout": 20000
	}]
}